{
  "name": "ndashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate": "drizzle-kit generate",
    "migrate": "drizzle-kit migrate",
    "postinstall": "prisma generate",
    "type": "tsc --noEmit",
    "prisma-seed": "tsx prisma/seed.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,css, tsx, ts}\"",
    "check": "npm run format && npm run lint && npm run type && vitest run",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test-ui": "vitest --ui"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@prisma/client": "^6.1.0",
    "@tabler/icons-react": "^3.18.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.3",
    "fastest-levenshtein": "^1.0.16",
    "js-cookie": "^3.0.5",
    "lucia": "^3.2.2",
    "next": "^15.1.6",
    "next-auth": "^5.0.0-beta.25",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.0",
    "tailwind-merge": "^2.5.3",
    "tsx": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@storybook/addon-controls": "^8.3.5",
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/addon-interactions": "^8.3.1",
    "@storybook/addon-links": "^8.3.1",
    "@storybook/addon-onboarding": "^8.3.1",
    "@storybook/addon-themes": "^8.3.3",
    "@storybook/blocks": "^8.3.1",
    "@storybook/nextjs": "^8.3.1",
    "@storybook/react": "^8.3.1",
    "@storybook/test": "^8.3.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@vitest/ui": "^3.0.4",
    "drizzle-kit": "^0.28.1",
    "eslint": "^8",
    "eslint-config-next": "^15.1.6",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prisma": "^6.1.0",
    "storybook": "^8.3.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^3.0.4"
  },
  "overrides": {
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2"
  }
}
